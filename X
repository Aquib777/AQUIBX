CBRING.Name = "CBRING"
CBRING.Parent = MainFrame
CBRING.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
CBRING.BorderSizePixel = 0
CBRING.Position = UDim2.new(0.0674603209, 0, 0.770318031, 0)
CBRING.Size = UDim2.new(0, 124, 0, 35)
CBRING.Font = Enum.Font.DenkOne
CBRING.Text = "CBRING"
CBRING.TextColor3 = Color3.fromRGB(255, 255, 255)
CBRING.TextSize = 24.000
CBRING.TextWrapped = true

UICorner_2.Parent = CBRING

CBRING.MouseButton1Click:Connect(function()


	_G.Reach = 0
	_G.KeyBindHigher = "q"
	_G.KeyBindLower = "e"
	_G.ReachOff = false -- Set this to true if you want to disable the reach lol

	game:GetService"RunService".Stepped:Connect(function()
		if _G.ReachOff then return end  
		pcall(function()
			Sword = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Handle
			for i,v in pairs(game.Players:GetPlayers()) do 
				if v ~= game.Players.LocalPlayer and v.Character:FindFirstChild("Left Arm") then
					if (game.Players.LocalPlayer.Character.Torso.Position - v.Character.Torso.Position).Magnitude <= _G.Reach then
						v.Character['Left Arm']:BreakJoints()
						v.Character['Left Arm'].Transparency = 1
						v.Character['Left Arm'].CanCollide = false
						v.Character['Left Arm'].CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(1,0,-3.5)
						v.Character['Left Leg']:BreakJoints()
						v.Character['Left Leg'].Transparency = 1
						v.Character['Left Leg'].CanCollide = false
						v.Character['Left Leg'].CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(1,0,-3.5)
					end
				end
			end
		end)
	end)

	local Mouse = game.Players.LocalPlayer:GetMouse()
	Mouse.KeyDown:Connect(function(key)
		if key == _G.KeyBindHigher then
			_G.Reach = _G.Reach + 1 
			game.StarterGui:SetCore("SendNotification", {
				Title = "Universal Reach";
				Text = "Reach set to " .. _G.Reach;
				Icon = "";
				Duration = 1;})
		end
	end)


	local Mouse = game.Players.LocalPlayer:GetMouse()
	Mouse.KeyDown:Connect(function(key)
		if key == _G.KeyBindLower then
			_G.Reach = _G.Reach - 1 
			game.StarterGui:SetCore("SendNotification", {
				Title = "Universal Reach";
				Text = "Reach set to " .. _G.Reach;
				Icon = "";
				Duration = 1;})
		end
	end)


end)

_200STUD.Name = "200STUD"
_200STUD.Parent = MainFrame
_200STUD.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
_200STUD.BorderSizePixel = 0
_200STUD.Position = UDim2.new(0.376984119, 0, 0.770318031, 0)
_200STUD.Size = UDim2.new(0, 124, 0, 35)
_200STUD.Font = Enum.Font.DenkOne
_200STUD.Text = "200 STUD"
_200STUD.TextColor3 = Color3.fromRGB(255, 255, 255)
_200STUD.TextSize = 24.000
_200STUD.TextWrapped = true

UICorner_3.Parent = _200STUD

_200STUD.MouseButton1Click:Connect(function()
	local toggle = Enum.KeyCode.T
	setsimulationradius(math.huge,math.huge)
	local active = false
	local ignoreTeams = true
	local plr = game:GetService("Players").LocalPlayer


	local spoofcf = nil

	local mt = getrawmetatable(game)
	setreadonly(mt,false)
	local index = mt.__index

	mt.__index = newcclosure(function(t,i)
		if not checkcaller() and typeof(t) == "Instance" and t:IsA("BasePart") and tostring(t) == "Handle" and (tostring(i) == "CFrame" or tostring(i) == "Position") and spoofcf then
			local char = plr.Character
			if tostring(i) == "CFrame" then
				return spoofcf
			elseif tostring(i) == "Position" then
				return spoofcf.p
			end
		end
		return index(t,i)
	end)

	game:GetService("UserInputService").InputBegan:Connect(function(inp,gpe)
		if gpe then return end
		if inp.KeyCode == toggle then
			active = true
		end
	end)
	game:GetService("UserInputService").InputEnded:Connect(function(inp,gpe)
		if gpe then return end
		if inp.KeyCode == toggle then
			active = false
		end
	end)

	local function getClosestCharacter()
		local closest = nil
		local lowest = 99999
		local hrp = plr.Character and plr.Character.PrimaryPart
		for _,v in pairs(game:GetService("Players"):GetPlayers()) do
			if v ~= plr then
				local hum = v.Character and v.Character:FindFirstChildOfClass("Humanoid")
				--if v.Team ~= plr.Team and hum and hum.Health < 10000 then
				local vhrp = v.Character and v.Character.PrimaryPart
				local hum = v.Character and v.Character:FindFirstChildOfClass("Humanoid")
				if vhrp and hum and hum.Health > 0 then
					local dist = (vhrp.Position-hrp.Position).Magnitude
					if dist < lowest then
						lowest = dist
						closest = v.Character
					end
				end
				--end
			end
		end
		return closest
	end

	local t = tick()
	game:GetService("RunService").RenderStepped:Connect(function()
		if plr.Character then
			local rightArm = plr.Character:FindFirstChild("Right Arm")
			local rightGrip = rightArm and rightArm:FindFirstChild("RightGrip")
			local s = plr.Character:FindFirstChildOfClass("Tool") or plr.Backpack:FindFirstChildOfClass("Tool")
			local h = s and s:FindFirstChild("Handle")
			if h and rightGrip then
				spoofcf = rightArm.CFrame*rightGrip.C0
				if not active then 
					rightGrip.Enabled = true
					return 
				end
				if tick()-t >= 0.05 then
					rightGrip.Enabled = false
					t = tick()
				elseif tick()-t >= 0.025 then
					rightGrip.Enabled = true
				end
				local target = plr:GetMouse().Target
				local p2 = target and target:FindFirstAncestorOfClass("Model") and game:GetService("Players"):GetPlayerFromCharacter(target:FindFirstAncestorOfClass("Model"))
				local char
				if p2 and p2.Character and p2.Character:FindFirstChildOfClass("Humanoid") and p2.Character:FindFirstChildOfClass("Humanoid").Health > 0 then
					char = p2.Character
				end
				char = char or getClosestCharacter()
				if char and spoofcf then
					h.CFrame = char.PrimaryPart and char.PrimaryPart.CFrame
				end
			else
				spoofcf = nil
			end
		end
	end)
	local function newChar(char)
		char.ChildAdded:Connect(function(s)
			if s:IsA("Tool") then
				local h = s:WaitForChild("Handle")
				if h then
					for _,v in pairs(getconnections(h.Changed)) do
						v:Disable()
					end
					for _,v in pairs(getconnections(h:GetPropertyChangedSignal("Position"))) do
						v:Disable()
					end
					for _,v in pairs(getconnections(h:GetPropertyChangedSignal("CFrame"))) do
						v:Disable()
					end
				end
			end
		end)
		char:WaitForChild("Right Arm").ChildAdded:Connect(function(c)
			if c:IsA("Weld") then
				for _,v in pairs(getconnections(c.Changed)) do
					v:Disable()
				end
				for _,v in pairs(getconnections(c:GetPropertyChangedSignal("Enabled"))) do
					v:Disable()
				end
			end
		end)
	end
	plr.CharacterAdded:Connect(newChar)
	if plr.Character then newChar(plr.Character) end
end)

HBE.Name = "HBE"
HBE.Parent = MainFrame
HBE.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
HBE.BorderSizePixel = 0
HBE.Position = UDim2.new(0.696428537, 0, 0.770318031, 0)
